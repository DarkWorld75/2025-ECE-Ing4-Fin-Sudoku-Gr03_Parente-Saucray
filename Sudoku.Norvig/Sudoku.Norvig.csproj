    <OutputType>Library</OutputType>
   <!-- <Nullable>enable</Nullable>-->
  </PropertyGroup>

  <ItemGroup>
   <!-- <Compile Include="NorvigSolver.cs" /> -->

    <ProjectReference Include="..\Sudoku.Shared\Sudoku.Shared.csproj" />
